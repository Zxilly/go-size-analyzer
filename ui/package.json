    "@types/lodash.memoize": "^4.1.9",
    "d3-array": "^3.2.4",
    "d3-color": "^3.1.0",
    "d3-hierarchy": "^3.1.2",
    "d3-scale": "^4.0.2",
    "lodash.memoize": "^4.1.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-dropzone": "^14.3.8",
    "react-use": "^17.6.0",
    "valibot": "^1.1.0"

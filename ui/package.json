    "sass": "^1.88.0",
    "terser": "^5.39.0",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vite-plugin-html": "^3.2.2",
    "vite-plugin-singlefile": "^2.2.0",
    "vitest": "^3.1.3"
  },
  "pnpm": {
    "peerDependencyRules": {
      "allowedVersions": {
        "eslint": "9"
      }
    },
    "overrides": {
      "micromatch": "4.0.8"
    }
  }
